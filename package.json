{
  "name": "node-mail-merge",
  "version": "0.0.4",
  "description": "mass mail from a csv",
  "main": "dist/index.js",
  "bin": "bin/cli.js",
  "directories": {
    "lib": "lib",
    "__tests__": "__tests__"
  },
  "files": [
    "package.json",
    "dist/",
    "bin/",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "nodemon --exec \"npm test\" --watch lib --watch __tests__",
    "lint": "eslint lib __tests__ bin",
    "prepush": "npm test && npm run lint",
    "preversion": "npm test",
    "postversion": "git push && git push --tags && npm publish",
    "exec": "./bin/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sam-parsons/node-mail-merge.git"
  },
  "author": "Sam Parsons <samparsons269@gmail.com> (http://sam-parsons.github.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sam-parsons/node-mail-merge/issues"
  },
  "homepage": "https://github.com/sam-parsons/node-mail-merge#readme",
  "devDependencies": {
    "@babel/core": "7.13.10",
    "@babel/preset-env": "7.13.10",
    "@typescript-eslint/eslint-plugin": "4.17.0",
    "@typescript-eslint/parser": "4.17.0",
    "babel-jest": "26.6.3",
    "babel-loader": "8.0.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "8.1.0",
    "jest": "26.6.3",
    "nodemon": "^1.11.0",
    "prettier": "2.2.1",
    "typescript": "4.2.3"
  }
}
