{
  "name": "node-mail-merge",
  "version": "0.0.0",
  "description": "mass mail from a csv",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib",
    "__tests__": "__tests__"
  },
  "scripts": {
    "build": "webpack",
    "test": "jest",
    "test:watch": "nodemon --exec \"npm test\" --watch lib --watch __tests__",
    "lint": "eslint lib test webpack.*",
    "prepush": "npm test && npm run lint",
    "preversion": "npm test",
    "postversion": "git push && git push --tags && npm publish",
    "exec": "./bin/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sam-parsons/node-mail-merge.git"
  },
  "author": "Sam Parsons <samparsons269@gmail.com> (http://sam-parsons.github.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sam-parsons/node-mail-merge/issues"
  },
  "homepage": "https://github.com/sam-parsons/node-mail-merge#readme",
  "dependencies": {
    "commander": "^2.10.0",
    "get-stdin": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.13.10",
    "@babel/preset-env": "7.13.10",
    "babel-core": "^6.25.0",
    "babel-jest": "26.6.3",
    "babel-loader": "8.0.0",
    "babel-preset-es2015": "6.24.1",
    "eslint": "^5.5.0",
    "jest": "26.6.3",
    "nodemon": "^1.11.0",
    "webpack": "^4.19.0",
    "webpack-cli": "^3.1.0"
  }
